#!/bin/bash

CLANG_BIN_PATH="../esp-clang/bin"

ARGS=""
while [[ $# > 0 ]] ; do
    case $1 in
        -dead_strip*)
        ;;
        -F*)
        ;;
        -filelist*)
        while read -r line; do
            ARGS=$ARGS" "$line
        done < $2
        shift ;;
        -no_deduplicate*)
        ;;
        -reproducible*)
        ;;
        *)
        ARGS=$ARGS" "$1
        ;;
    esac
    shift
done

echo $CLANG_BIN_PATH/riscv32-esp-elf-clang-ld $ARGS
$CLANG_BIN_PATH/riscv32-esp-elf-clang-ld $ARGS
exit $?
